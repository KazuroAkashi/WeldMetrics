{
  "name": "local-database-container",
  "private": true,
  "type": "commonjs",
  "main": "electron/main.mjs",
  "version": "0.0.0",
  "description": "A local database container with sqlite3",
  "author": "KazuroAkashi",
  "repository": {
    "type": "git",
    "url": "https://github.com/KazuroAkashi/local-database-container.git"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "services": "node gen-services.cjs",
    "event-listener": "node gen-event-listener.cjs",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/publisher-github": "^7.4.0",
    "@nuxt/kit": "3.6.5",
    "@types/github-url-to-object": "^4.0.4",
    "@types/is-url": "^1.2.32",
    "@types/ms": "^0.7.34",
    "@types/uuid": "^8.3.4",
    "cypress": "^10.2.0",
    "electron": "^31.3.1",
    "gulp": "^5.0.0",
    "nuxt": "3.6.5",
    "nuxt-electron": "^0.7.0",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.3.13",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "@pinia/nuxt": "^0.5.1",
    "electron-squirrel-startup": "^1.0.1",
    "github-url-to-object": "^4.0.6",
    "is-url": "^1.2.4",
    "ms": "^2.1.3",
    "pinia": "^2.1.7",
    "sass": "^1.68.0",
    "scss": "^0.2.4",
    "sqlite3": "^5.1.7",
    "uuid": "^8.3.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel"
        },
        {
          "name": "@electron-forge/maker-deb"
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "KazuroAkashi",
              "name": "local-database-container"
            },
            "draft": true,
            "force": true
          }
        }
      ]
    }
  }
}
